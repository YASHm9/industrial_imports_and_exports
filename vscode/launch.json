{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Built-in PHP Web Server",
            "type": "php",
            "request": "launch",
            "runtimeArgs": [
                "-S",
                "localhost:8080",
                "-t",
                "${workspaceFolder}"
            ],
            "webRoot": "${workspaceFolder}",
            "port": 8080
        }
    ]
}
